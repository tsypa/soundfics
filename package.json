  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tsypa/soundfics/issues"
  },
  "homepage": "https://github.com/tsypa/soundfics#readme",
  "dependencies": {
    "lodash": "^4.5.0",
    "node-aplay": "^1.0.3"
  }
}
